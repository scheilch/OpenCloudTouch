{
  "name": "cloudtouch-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "CloudTouch - Local control for Bose SoundTouch devices",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode development",
    "dev:live": "VITE_MOCK_MODE=false vite",
    "build": "vite build",
    "build:mock": "vite build --mode development",
    "preview": "vite preview",
    "preview:mock": "vite preview --mode development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:real": "vitest run tests/real",
    "test:e2e:mock": "powershell ../scripts/run-e2e-tests.ps1 -MockMode $true",
    "test:e2e:real": "cypress run --spec 'tests/real/**/*.cy.js'",
    "test:e2e:real:debug": "cypress open --config specPattern='tests/real/**/*.cy.js'",
    "test:e2e:debug": "powershell ../scripts/run-e2e-tests.ps1 -MockMode $true -HeadlessMode $false",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "framer-motion": "^12.31.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^1.0.0",
    "@eslint/markdown": "^7.5.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "cypress": "^13.6.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
