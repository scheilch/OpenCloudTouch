{
  "name": "opencloudtouch-frontend",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "description": "OpenCloudTouch - Local control for compatible streaming devices",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run --browser chrome --headless",
    "test:e2e:headed": "cypress run --headed --browser chrome",
    "test:e2e:open": "cypress open",
    "lint": "eslint .",
    "clean": "rimraf node_modules dist .vite coverage cypress/screenshots cypress/videos"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "cypress": "^15.10.0",
    "framer-motion": "^12.33.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "9.39.2",
    "@eslint/json": "^1.0.0",
    "@eslint/markdown": "^7.5.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "colorette": "^2.0.20",
    "eslint": "9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
