# Example ENV file for SoundTouchBridge
# Copy to .env and adjust values

STB_HOST=0.0.0.0
STB_PORT=8000
STB_LOG_LEVEL=INFO

STB_DB_PATH=/data/stb.db

STB_DISCOVERY_ENABLED=true
STB_DISCOVERY_TIMEOUT=10
# STB_MANUAL_DEVICE_IPS=192.168.1.100,192.168.1.101

STB_SOUNDTOUCH_HTTP_PORT=8090
STB_SOUNDTOUCH_WS_PORT=8080

# Adjust to your STB host IP/hostname
STB_STATION_DESCRIPTOR_BASE_URL=http://192.168.1.50:8000/stations/preset
