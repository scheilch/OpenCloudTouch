# Example ENV file for CloudTouch
# Copy to .env and adjust values

CT_HOST=0.0.0.0
CT_PORT=7777
CT_LOG_LEVEL=INFO

CT_DB_PATH=/data/ct.db

CT_DISCOVERY_ENABLED=true
CT_DISCOVERY_TIMEOUT=10
# CT_MANUAL_DEVICE_IPS=192.168.1.100,192.168.1.101

CT_SOUNDTOUCH_HTTP_PORT=8090
CT_SOUNDTOUCH_WS_PORT=8080

# Adjust to your CT host IP/hostname
CT_STATION_DESCRIPTOR_BASE_URL=http://192.168.1.50:7777/stations/preset
